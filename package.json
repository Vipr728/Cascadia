{
  "name": "cascadia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "voice": "tsx voice-ai-server.ts",
    "voice-server": "tsx voice-websocket-server.ts",
    "voice-dev": "concurrently \"next dev\" \"tsx voice-websocket-server.ts\"",
    "py:install": "python -m venv .venv && .venv/Scripts/pip install -r agents/requirements.txt || .venv/bin/pip install -r agents/requirements.txt",
    "analysis:test": "powershell -Command \"Invoke-RestMethod -Uri 'http://localhost:3000/api/analysis' -Method POST -ContentType 'application/json' -Body '{\\\"transcript\\\": \\\"I has a apple yesterday. It are very good.\\\", \\\"language\\\": \\\"english\\\"}' | ConvertTo-Json -Depth 6\""
  },
  "dependencies": {
    "@fastify/formbody": "^8.0.2",
    "@fastify/websocket": "^11.2.0",
    "@google/genai": "^1.20.0",
    "@google/generative-ai": "^0.24.1",
    "@types/express": "^5.0.3",
    "@types/ws": "^8.18.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "fastify": "^5.6.0",
    "next": "15.5.3",
    "openai": "^4.59.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "twilio": "^5.10.0",
    "undici": "^7.16.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
